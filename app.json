{
  "expo": {
    "name": "Namaz Timings",
    "slug": "NamazTimings",
    "version": "1.1.0",
    "orientation": "portrait",
    "plugins": [
      "@react-native-firebase/app",
      "expo-location"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.namaztimings",
      "googleServicesFile": "./google-services.json",
      "jsEngine": "hermes"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
